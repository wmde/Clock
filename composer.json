{
	"name": "wmde/clock",
	"license": "BSD-3-Clause",
	"description": "Simple interface to get the current time without binding to global system resources. Includes test doubles!",
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.1",
		"phpstan/phpstan": "~0.12."
	},
	"autoload": {
		"psr-4": {
			"WMDE\\Clock\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WMDE\\Clock\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
